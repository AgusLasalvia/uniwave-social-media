{
	"cli": {
		"version": ">= 16.17.3",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal"
		},
		"preview": {
			"distribution": "internal"
		},
		"production": {
			"autoIncrement": true
		},
		"apk": {
			"android": {
				"buildType": "apk"
			},
			"distribution": "internal",
			"env": {
				"EXPO_PUBLIC_API_URL": "https://api.example.com" 
			}
		}
	},
	"submit": {
		"production": {}
	}
}